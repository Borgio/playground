#!/bin/bash

[ -f $(dirname $0)/common ] && . $(dirname $0)/common || { echo "$(dirname $0)/common not found" 1>&2; exit 1; }

function display-help()
{
cat <<EOF

Usage:

     ./bin/send \\
       --jndi 127.0.0.1:4447 \\
       --destination /topic/novaordis \\
       --connection-factory /RemoteConnectionFactory \\
       --messages 1000 \\
       --threads 10

EOF
}

operation=send

main $@