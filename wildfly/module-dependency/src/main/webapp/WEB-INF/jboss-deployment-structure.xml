<!--
Explicit dependencies declared here can be replaced with a "global" module:

<subsystem xmlns="urn:jboss:domain:ee:1.2">
  <global-modules>
    <module name="io.novaordis.playground.wildfly.custommodule" slot="1.0" />
  </global-modules>
  ...
</subsystem>
-->

<jboss-deployment-structure>
    <deployment>
        <dependencies>
            <module name="io.novaordis.playground.wildfly.custommodule" slot="1.0"/>
        </dependencies>
    </deployment>
</jboss-deployment-structure>